<section id="users">
  <div class="container">
    <h2 class="m-heading text-primary my-1">All Users</h2>
    {{#each users}}
    <div class="user py-1">
      <span class="right sm-heading"><i class="far fa-user p-1"></i> {{name}}
      </span>

      <a href="/api/users/{{id}}" class="btn btn-light-inverse"><i class="fas fa-arrow-right fa-2x"></i></a>
    </div>
    {{/each}}
  </div>
</section>
{{> _footer}}